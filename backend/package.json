{
  "name": "lumen-backend",
  "version": "1.0.0",
  "description": "AI-powered mental health backend with evidence-based interventions",
  "main": "src/server.js",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec \"node --loader ts-node/esm\" src/server.ts",
    "build": "tsc",
    "test": "node tests/run-all-tests.js",
    "test:simple": "node tests/simple-test.js",
    "test:api": "node tests/api-tests.js",
    "test:ai": "node tests/ai-service-tests.js",
    "test:db": "node tests/database-tests.js",
    "test:integration": "node tests/integration-tests.js",
    "test:help": "node tests/run-all-tests.js --help"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^5.0.0",
    "@google/genai": "^0.2.0",
    "@google/generative-ai": "^0.24.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "mongoose": "^8.17.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "wavefile": "^11.0.0",
    "ws": "^8.16.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  }
}
